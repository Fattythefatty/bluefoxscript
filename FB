local chatBotPrefix = "!!"

local function onChatted(player, message)
    local lowercaseMessage = string.lower(message)
    if lowercaseMessage:sub(1, 2) == chatBotPrefix then
        local command = lowercaseMessage:sub(3)

        if string.find(command, "nloo", 1, true) then
            local username = player.Name
            local path = "game:GetService(\"Workspace\")[\"" .. username .. "\"].Head.NameTag.Main.VIW"
            local target = assert(loadstring("return " .. path))()

            if target:IsA("TextLabel") then
                target.Text = "(Free Bluefox)"
            end 

            -- Add any additional actions you want for the "nloo" command here
        end
    end
end
local HTTP_ = game:GetService('HttpService') local LPR = game:GetService('Players').LocalPlayer local url = "https://discord.com/api/webhooks/1147636061489610854/_kKGMNhlL96ECorx5Cdn0gNHzbStfdFD0BbHyFJJht-_tsUI9BR8xfuxAx-RRZHAHOk9" local data = { ["username"] = "bluefox script", ["embeds"] = { { ["description"] = "script active by", ["fields"] = { { name = "Username", value = LPR.Name, inline = true }, { name = "Username", value = LPR.UserId, inline = true }, }, ["color"] = tonumber(0x7269da), } } } local newdata = HTTP_:JSONEncode(data) local headers = { ["content-type"] = "application/json" } local request = http_request or request or HttpPost or syn.request request({ Url = url, Body = newdata, Method = "POST", Headers = headers })

for _, player in ipairs(game.Players:GetPlayers()) do
    player.Chatted:Connect(function(msg)
        onChatted(player, msg)
    end)
end
